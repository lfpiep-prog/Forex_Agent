# ============================================
# Forex Agent - Environment Variables Template
# ============================================
# Copy this file to .env and fill in your values.
# DO NOT COMMIT .env with real keys!

# --- IG Markets Broker Credentials ---
IG_USERNAME=your_ig_username
IG_PASSWORD=your_ig_password
IG_API_KEY=your_ig_api_key
IG_ACC_TYPE=DEMO  # Options: DEMO or LIVE

# --- Brave Search API (for Sentiment Analysis) ---
BRAVE_API_KEY=your_brave_api_key

# --- Twelve Data Creds ---
TWELVEDATA_API_KEY=your_twelvedata_key_here

# --- Optional: Legacy Keys (if using other services) ---
# BROKER_API_KEY=
# MARKET_DATA_KEY=

